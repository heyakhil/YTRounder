{
  "manifest_version": 3,
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "home/index.html"
  },
  "host_permissions": ["http://*/*", "https://*/*"],
  "permissions": [
    "tabs",
    "scripting", 
    "activeTab",
    "storage"
  ]
}